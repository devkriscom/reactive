	<ul class="nav ml-auto mr-20">
		
		{% for item in menu %}
			{% if item.childrens|length > 0 %}
				<li class="nav-item dropdown">
					<a class="nav-link dropdown-toggle" href="{{item.path}}" id="dropdown{{item.id}}" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">{{item.title}}</a>
					<div class="dropdown-menu" aria-labelledby="dropdown{{item.id}}">
						<ul class="subnav">
							{% for child in item.childrens %}
								{% if child.childrens|length > 0 %}
									<li class="nav-item second-level dropdown-submenu">
										<a class="dropdown-item dropdown-toggle" href="{{child.path}}" id="dropdown-{{child.id}}" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
											{{child.title}}
										</a>
										<div class="dropdown-menu" aria-labelledby="dropdown-{{child.id}}">
											<ul class="nav">
												{% for subchild in child.childrens %}
													<li class="nav-item thrid-level">
														<a class="dropdown-item" href="{{subchild.path}}">
															{{subchild.title}}</a>
													</li>
												{% endfor %}
											</ul>
										</div>
									</li>
								{% else %}
									<li class="nav-item second-level">
										<a class="dropdown-item" href="{{child.path}}">{{child.title}}</a>
									</li>
								{% endif %}
							{% endfor %}
						</ul>
					</div>
				</li>
			{% else %}
				<li class="nav-item">
					<a class="nav-link" href="{{item.path}}" id="{{item.id}}">{{item.title}}</a>
				</li>
			{% endif %}
		{% endfor %}
	</ul>